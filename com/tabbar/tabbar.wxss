/* com/tabbar/tabbar.wxss */
/* 使用自定义tabbar时，防止下层内容被盖住 */
.index_main{
  padding-bottom: 94rpx;
}
.com-tabbar{
  position:fixed;
  bottom:0;
  display:flex;
  align-items:center;
  width:100%;
  height:94rpx;
  box-sizing:border-box;
  border-top:1rpx solid;
  text-align:center;
  font-size:22rpx;
}
.com-tabbar .tabbarItem{
  flex:1;
  height:inherit;
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content:center;
}
.com-tabbar .tabbarItem .icon{
  width:50rpx;
  height:50rpx;
}
